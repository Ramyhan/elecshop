<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.kh.elecshop.mapper.ProductOptionMapper">

	<select id="selectOption" resultType="com.kh.elecshop.domain.ProductOptionVO">
		select *
		from tbl_product_option
		where pno = #{pno}
	</select>
	
</mapper>